{
  "name": "Travel",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mysql": {
    "table": "e_travel"
  },
  "properties": {
    "distance": {
      "type": "number",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "car": {
      "type": "belongsTo",
      "model": "Car",
      "foreignKey": "carId"
    },
    "driver": {
      "type": "belongsTo",
      "model": "Driver",
      "foreignKey": "driverId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "checkCarAuth",
      "permission": "ALLOW",
      "property": "add"
    }
  ],
  "methods": {}
}
