{
  "name": "Car",
  "plural": "cars",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mysql": {
    "table": "e_car"
  },
  "properties": {
    "registrationNumber": {
      "type": "string",
      "required": true
    },
    "color": {
      "type": "string",
      "required": true
    },
    "lastTimeUsed": {
      "type": "date"
    },
    "traveledDistance": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "drivers": {
      "type": "hasAndBelongsToMany",
      "model": "Driver",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "carOwner",
      "permission": "ALLOW",
      "property": "addDistance"
    }
  ],
  "methods": {}
}
